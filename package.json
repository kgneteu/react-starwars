{
  "name": "react-html5",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "src/**/*.js",
      "src/**/*.ts",
      "!**/node_modules/**"
    ]
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "serve build": "serve -s build",
    "test": "craco test",
    "coverage": "craco test --coverage --watchAll",
    "e2e": "playwright test --project=chromium --headed --config ./e2e/config/playwright.config.js",
    "copy SWAPI": "node tools/copy-sw-api.js",
    "source map explorer": "craco build && source-map-explorer 'build/static/js/*.js'",
    "webpack bundle anlyzer": "craco build --stats && webpack-bundle-analyzer build/bundle-stats.json",
    "predeploy": "craco build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-responsive": "^9.0.0-beta.4",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "react-toastify": "^8.1.0",
    "react-transition-group": "^4.4.5",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.24.2",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@craco/craco": "^6.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^12.1.10",
    "@types/prop-types": "^15.7.5",
    "@types/react-transition-group": "^4.4.5",
    "typescript": "^4.7.4",
    "autoprefixer": "^9.8.8",
    "gh-pages": "^3.2.3",
    "playwright": "^1.24.2",
    "postcss": "^7.0.39",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.43.4",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
